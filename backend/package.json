{
  "name": "pathfinding-a-star",
  "version": "0.0.1",
  "description": "Pathfinding A*",
  "main": "application.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "PORT=3000 NODE_ENV=development nodemon --watch src --ext ts --exec ts-node src/application.ts",
    "debug": "nodemon --inspect=0.0.0.0:3000 --exec 'ts-node src/application.ts'",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/application.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelthomasjach/pathfinding-a-star.git"
  },
  "author": "michaelthomasjach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelthomasjach/pathfinding-a-star/issues"
  },
  "homepage": "https://github.com/michaelthomasjach/pathfinding-a-star#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0"
  }
}
